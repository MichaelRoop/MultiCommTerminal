<local:UC_UintEditBoxBase x:Class="MultiCommTerminal.NetCore.UserControls.EditBoxes.UC_UIntEditBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MultiCommTerminal.NetCore.UserControls.EditBoxes"
             mc:Ignorable="d" >
    <Grid HorizontalAlignment="Left">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="tbEdit" Style="{StaticResource TxtBoxLeft}"  Margin="0"
                 PreviewKeyDown="tbEdit_PreviewKeyDown" TextChanged="tbEdit_TextChanged" />
    </Grid>
</local:UC_UintEditBoxBase>
